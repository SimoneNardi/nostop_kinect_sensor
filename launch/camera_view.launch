<!-- to view image-->
<launch>

    
  <node name="camera" pkg="usb_cam" type="usb_cam_node" output="screen">
  	<param name="video_device" value="/dev/video1" />
  	<param name="image_width" value="320" />
    <param name="image_height" value="240" />
  	<param name="pixel_format" value="yuyv" />
  </node> 
  
</launch>
